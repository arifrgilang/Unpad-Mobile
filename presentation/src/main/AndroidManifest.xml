<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.arifrgilang.presentation">

    <application>
        <activity android:name=".pauslogin.PausLoginActivity"></activity>
        <activity android:name=".webview.WebViewActivity" />
        <activity android:name=".dashboard.DashboardActivity" />
        <activity
            android:name=".main.MainActivity"
            android:theme="@style/Theme.UnpadMobile.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data
                    android:host="jendela.unpad.ac.id"
                    android:scheme="http" />
            </intent-filter>
        </activity>
    </application>

</manifest>